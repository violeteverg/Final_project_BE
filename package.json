{
  "name": "final_project_be",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm run serve",
    "dev": "concurrently \"npm run build:watch\" \"npm run copy-files\" \"npm run serve:watch\"",
    "copy-files": "cp -r ./views ./dist/",
    "serve": "node dist/index.js",
    "serve:watch": "nodemon index.js",
    "build": "tsc",
    "build:watch": "tsc -w",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo:all",
    "seed:generate": "npx sequelize-cli seed:generate --name",
    "seed:run": "npx sequelize-cli db:seed --seed"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.7.7",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "cloudinary": "^2.5.1",
    "concurrently": "^9.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.7.0",
    "handlebars": "^4.7.8",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.11.3",
    "nodemailer": "^6.9.15",
    "nodemon": "^3.1.7",
    "sequelize": "^6.37.4"
  },
  "devDependencies": {
    "sequelize-cli": "^6.6.2"
  }
}
